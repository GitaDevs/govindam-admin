{
  "kind": "collectionType",
  "collectionName": "user_subscriptions",
  "info": {
    "singularName": "user-subscription",
    "pluralName": "user-subscriptions",
    "displayName": "UserSubscription"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "valid_from": {
      "type": "datetime",
      "required": true
    },
    "valid_till": {
      "type": "datetime",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription"
    }
  }
}
